<%= link_to("<< Back to List",{:action => 'index'}, :class => 'back-link') %>
<div class="subjects show">
  <h2>Show Subject</h2>

  <table summary="Subject detail view">
    <tr>
      <th>Name</th>
      <td><%= @subject.name %>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @subject.position %>
    </tr>
    <tr>
      <th>Visible?</th>
      <td><%= @subject.visible ? 'true' : 'false' %>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @subject.created_at %>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @subject.updated_at %>
    </tr>
</table>
