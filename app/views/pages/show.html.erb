<% if !flash[:notice].blank? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<% end %>
<%= link_to("<< Back to List",{:action => 'index'}, :class => 'back-link') %>
<div class="pages show">
  <h2>Show Page</h2>

  <table summary="page detail view">
    <tr>
      <th>Subject_ID</th>
      <td><%= @page.subject_id %>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @page.name %>
    </tr>
    <tr>
   <tr>
      <th>Permalink</th>
      <td><%= @page.permalink %>
    </tr>
      <th>Position</th>
      <td><%= @page.position %>
    </tr>
    <tr>
      <th>Visible?</th>
      <td><%= @page.visible ? 'true' : 'false' %>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @page.created_at %>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @page.updated_at %>
    </tr>
</table>
